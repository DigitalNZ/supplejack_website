{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "build:dev:client": "node_modules/.bin/webpack -w --config webpack.client.rails.config.js",
    "build:production:client": "NODE_ENV=production node_modules/.bin/webpack --config webpack.client.rails.build.config.js",
    "test": "NODE_ENV=test mocha \"app/**/*-test.jsx\" --compilers js:babel-core/register --recursive --watch app/",
    "test:watch": "NODE_ENV=test npm test -- --watch"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5",
    "es5-shim": "^4.5.7",
    "jquery": "^2.2.2",
    "jquery-ujs": "^1.2.1",
    "loader-utils": "^0.2.13",
    "lodash": "^4.7.0",
    "react": "^0.14.8 || ^15.0.0",
    "react-dom": "^0.14.8 || ^15.0.0",
    "react-on-rails": "6.0.0-rc.1",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "expose-loader": "^0.7.1",
    "imports-loader": "^0.6.5",
    "webpack": "^1.12.14"
  }
}
